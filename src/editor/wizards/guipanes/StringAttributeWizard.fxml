<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<SplitPane fx:id="root" fx:controller="editor.wizards.StringAttributeWizard"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<items>		
		<AnchorPane>
			<children>
				<VBox>
					<ComboBox fx:id="key" editable="true" promptText="Attribute Name" />
					<TextField fx:id="stringValue" />
					<Button fx:id="save" defaultButton="true" text="save" />
					<Text fx:id="errorMessage" />
				</VBox>
			</children>
		</AnchorPane>
	</items>
</SplitPane>
