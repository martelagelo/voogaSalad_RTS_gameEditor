<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<SplitPane fx:id="root" fx:controller="editor.wizards.TerrainWizard"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<items>
		<AnchorPane>
			<children>
				<VBox>
					<TextField fx:id="name" text="name" />
					<Button fx:id="trigger" defaultButton="true" text="new trigger" />
					<Button fx:id="stringAttribute" defaultButton="true" text="new string attribute" />
					<Button fx:id="numberAttribute" defaultButton="true" text="new number attribute" />
					<Button fx:id="save" defaultButton="true" text="save" />
					<Text fx:id="errorMessage" />

				</VBox>
			</children>
		</AnchorPane>
		<AnchorPane>
			<children>
				<ImageView fx:id="spritesheet" />
			</children>
		</AnchorPane>
	</items>
</SplitPane>
