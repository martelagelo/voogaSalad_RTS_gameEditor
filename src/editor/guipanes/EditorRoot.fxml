<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import editor.EditorMenuBar?>

<fx:root type="javafx.scene.layout.BorderPane" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1">
	<top>
		<EditorMenuBar/>
	</top>
	<left>
		<VBox BorderPane.alignment="CENTER">
			<children>
				<TreeView fx:id="rootViewProjectExplorer" VBox.vgrow="ALWAYS" />
				<VBox fx:id="rootViewGameInfoVBox" VBox.vgrow="NEVER" />
			</children>
		</VBox>
	</left>
	<center>
		<TabPane fx:id="rootViewTabPane" BorderPane.alignment="CENTER" />
	</center>
</fx:root>
