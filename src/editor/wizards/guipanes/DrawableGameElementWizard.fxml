<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.*?>

<SplitPane fx:id="root" fx:controller="editor.wizards.DrawableGameElementWizard"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<items>
		<AnchorPane>
			<children>
				<VBox>
					<TextField fx:id="name" />
					<Button fx:id="trigger" />
					<Button fx:id="stringAttribute" />
					<Button fx:id="numberAttribute" />
					<Button fx:id="image" />
					<HBox>
						<Slider fx:id="frameWidth" />
						<TextField fx:id="frameWidthText" />
					</HBox>
					<HBox>
						<Slider fx:id="frameHeight" />
						<TextField fx:id="frameHeightText" />
					</HBox>
					<TextField fx:id="numRows" />
					<TextField fx:id="startFrame" />
					<TextField fx:id="stopFrame" />
					<CheckBox fx:id="animationRepeat" />
					<Button fx:id="save" />
					<Text fx:id="errorMessage" />
				</VBox>
			</children>
		</AnchorPane>
		<ScrollPane fx:id="imageScroll">
			<Group fx:id="spritesheet" />
		</ScrollPane>
	</items>
</SplitPane>
